            <input class="gsearch" type="search" name="searchList" value="" />
            <span class="placeholder platform-font-2" data-full-text="Simply Search" data-short-text="SEARCH"></span>
            <span class="glyph-replace focus" data-glyph="&#99;"></span>
            <span class="glyph-replace clear" data-glyph="&#79;"></span>
            </label>
            </form>
            </div>
            <div class="home facet nav-section l-cell">
           
              
          <strong style="color:#dc2028;"><%= link_to('hindi',{ :action => 'index', :lang => 'hindi'}) %></strong> 
       
            </div>
            
            <div class="home facet nav-section l-cell">
            <a href="#"><strong>Login</strong></a>
            </div>
            </div>
            <div class="tier1 l-fullwidth">
            <div data-subnav="shop"
