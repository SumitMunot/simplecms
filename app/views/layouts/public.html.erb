  <body  onLoad="MM_preloadImages('/assets/social/facebook-hover.png','/assets/social/youtube-hover.png','/assets/social/twitter-hover.png','/assets/social/linkedIn-hover.png','/assets/social/google-hover.png','/assets/social/vimeo-hover.png')">
<div data-gadget="simplyK.gadget.OnesimplyKNav" data-load="preloaded" data-search-redirect="false">
            <nav class="gnav platform-font-1">
            <div class="tier0 l-table l-fullwidth">
            <div class="home facet nav-section l-cell">
            <%= link_to image_tag("logo.jpg", :width => '312', :height => '33'), :action => 'index' %>
            </div>
