            <div class="subnav-container">
            <div class="subnav-section l-table">

                 <%= render(:partial => 'layouts/navigation', :locals => {:subjects => @subjects}) %>
            </div>
            </div>
            </div>
            <div data-subnav="sports"
            class="subnav sports tracking is-hidden"
            data-nav-tracking="sports">
            <div class="subnav-container">
            <div class="subnav-section l-table">
            <div class="subnav-item l-cell l-w6 visual-format">
            <a data-subnav-label="RUNNING"
            href="#"
            data-nav-tracking="running"
            data-track-click="true">
            <%= image_tag("simply_images/videos.png", :width => '100', :height => '100') %>
            <div class="subnav-itemheader">VIDEO</div>
            </a>
            </div>
            <div class="subnav-item l-cell l-w6 visual-format">
            <a data-subnav-label="BASKETBALL"
            href="#"
            data-nav-tracking="basketball"
            data-track-click="true">
            <%= image_tag("simply_images/audios.png", :width => '100', :height => '100') %>
            <div class="subnav-itemheader">AUDIO</div>
            </a>
            </div>
            <div class="subnav-item l-cell l-w6 visual-format">
            <a data-subnav-label="FOOTBALL"
            href="#"
            data-nav-tracking="football"
            data-track-click="true">
            <%= image_tag("simply_images/books.png", :width => '100', :height => '100') %>
            <div class="subnav-itemheader">BOOKS</div>
            </a>
            </div>
            <div class="subnav-item l-cell l-w6 visual-format">
            <a data-subnav-label="SOCCER"
            href="#"
